{
  "name": "atk4/audit",
  "type": "library",
  "description": "Agile Data - Audit Extension",
  "keywords": [
    "agile",
    "data",
    "framework",
    "audit",
    "event sourcing",
    "crud",
    "acl"
  ],
  "homepage": "http://agiletoolkit.org/data/extension/audit",
  "license": "MIT",
  "authors": [
    {
      "name": "Romans Malinovskis",
      "email": "romans@agiletoolkit.org",
      "homepage": "https://nearly.guru/"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.3.0",
    "atk4/data": "dev-develop",
    "ext-json": "*"
  },
  "require-release": {
    "php": ">=7.3.0",
    "atk4/data": "2.1.*",
    "ext-json": "*"
  },
  "require-dev": {
    "atk4/ui": "dev-develop",
    "atk4/schema": "dev-develop",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpcov": "*",
    "phpunit/phpunit": ">=9.0"
  },
  "require-dev-release": {
    "atk4/ui": "2.1.*",
    "atk4/schema": "2.1.*",
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpcov": "*",
    "phpunit/phpunit": ">=9.0"
  },
  "suggest": {
    "atk4/ui": "Provides you with extra Views to display history",
    "atk4/mastercrud": "Intergates with MasterCRUD showing change history of your records"
  },
  "autoload": {
    "psr-4": {
      "atk4\\audit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "atk4\\audit\\tests\\": "tests/"
    }
  }
}
